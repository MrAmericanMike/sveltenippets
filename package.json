{
	"name": "sveltesnippets",
	"displayName": "Svelte Snippets",
	"description": "Svelte Snippets",
	"version": "0.0.5",
	"license": "MIT",
	"publisher": "MrAmericanMike",
	"author": {
		"name": "MrAmericanMike"
	},
	"icon": "icons/logo.png",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"svelte",
		"snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/MrAmericanMike/sveltesnippets.git"
	},
	"bugs": {
		"url": "https://github.com/MrAmericanMike/sveltesnippets/issues"
	},
	"contributes": {
		"languages": [
			{
				"id": "svelte",
				"aliases": [
					"Svelte",
					"Svelte 3",
					"svelte"
				],
				"filenamePatterns": [
					"*.svelte"
				]
			}
		],
		"snippets": [
			{
				"language": "svelte",
				"path": "./snippets/svelte.code-snippets"
			},
			{
				"language": "javascript",
				"path": "./snippets/svelte-javascript.code-snippets"
			},
			{
				"language": "typescript",
				"path": "./snippets/svelte-javascript.code-snippets"
			},
			{
				"language": "css",
				"path": "./snippets/svelte-css.code-snippets"
			},
			{
				"language": "html",
				"path": "./snippets/svelte.code-snippets"
			}
		]
	}
}

